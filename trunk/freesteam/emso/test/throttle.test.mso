using "streams";
using "throttle";

FlowSheet TestThrottle
		
	DEVICES
	S1 as Stream_ph;
	TH as Throttle;
	
	CONNECTIONS
	S1 to TH.In;
	
	SPECIFY
	"Flow rate"
	S1.mdot = 27 * "kg/min";
	
	"Inlet pressure"
	S1.p = 50 * "bar";
	
	"Inlet enthalpy"
	S1.h = 3500 * "kJ/kg";
	
	"Output pressure"
	TH.Out.p = 65 * "bar";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end
