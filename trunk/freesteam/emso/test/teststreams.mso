using "streams";

FlowSheet Test_Stream_ps
    # fails to run properly
    
    PARAMETERS
    freesteam as CalcObject(File="emsofreesteam");
    
    DEVICES
    S as Stream_ps;
    
    SPECIFY
    "Flow rate"
    S.mdot = 10 * "kg/s";
    
    "Pressure"
    S.p = 101700 * "Pa";

    "Entropy"
    S.s = 7.5 * "kJ/kg/K";
    
    OPTIONS
    relativeAccuracy = 1e-10;
    mode = "steady";
    outputLevel = "all";
end


FlowSheet Test_Stream_ph
    # tested, runs OK.
    
    PARAMETERS
    freesteam as CalcObject(File="emsofreesteam");
    
    DEVICES
    S as Stream_ph;
    
    SPECIFY
    "Flow rate"
    S.mdot = 10 * "kg/s";
    
    "Pressure"
    S.p = 101700 * "Pa";

    "Enthalpy"
    S.h = 2500 * "kJ/kg";
    
    OPTIONS
    relativeAccuracy = 1e-8;
    mode = "steady";
    outputLevel = "all";
end

FlowSheet Test_Stream_waterT
    
    PARAMETERS
    freesteam as CalcObject(File="emsofreesteam");
    
    DEVICES
    S as Stream_waterT;
    
    SPECIFY
    "Flow rate"
    S.mdot = 10 * "kg/s";
    
    "Pressure"
    S.T = (273.15 + 250) * "K";
    
    OPTIONS
    relativeAccuracy = 1e-8;
    mode = "steady";
    outputLevel = "all";
end

