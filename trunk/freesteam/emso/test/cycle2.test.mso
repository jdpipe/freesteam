using "streams";
using "turbine";
using "boiler";
using "pump";
using "condenser";
using "throttle";

FlowSheet CycleTest
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
    DEVICES # 31 DF
    S1  as Stream_pT;  # 3
	BO	as Boiler_pT;     # 7
	TU	as Turbine;    # 7
	CO	as Condenser_pT;  # 7
	PU 	as Pump;       # 7
	
	CONNECTIONS # BO -> TU -> CO -> TH -> BO  ... -25 DF
	S1      to  BO.In; # -5
	BO.Out	to	TU.In; # -5
	TU.Out	to  CO.In; # -5 
	CO.Out	to  PU.In; # -5
	PU.Out	to  S1; # -5
	
	SPECIFY # -8 DF
	
	S1.mdot = 38 * "kg/s";
	#S1.p = 60 * "bar";
	S1.T = (121 + 273.15) * "K";	

	# Boiler
	#BO.Out.p = 58 * "bar";
	BO.Dp = -2 * "bar";
	BO.Out.T = (500 + 273.15) * "K";
	
	# Turbine
	TU.eta = 0.85;
	#TU.Out.p = 2.0 * "bar";
	
	# Condenser
	#CO.Out.p = 1.5 * "bar";
	CO.Out.T = (40 + 273.15) * "K";
	
	# Pump
	PU.eta = 0.65;
	#PU.Out.p = 60 * "bar";
	
	GUESS
	S1.p = 60 * "bar";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end
