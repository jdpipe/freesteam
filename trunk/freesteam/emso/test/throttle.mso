using "steamequipment";

#*
    Isenthalpic throttle
*#
Model Throttle as SteamEquipment_ph # 8 - 2 = 6
	
	EQUATIONS
	
	"Zero volume"
	H = 0  *"J";
	m = 0 * "kg";
	
	"Zero work"
	Wdot = 0 * "W";
	# Qdot = 0 * "W";
	
	# Isenthalpic
	In.h = Out.h;
	
end
