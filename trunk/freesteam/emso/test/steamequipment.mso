using "newtypes";
using "streams";

#** 
    Basic steam equipment item with one inlet and outlet stream.
*#
Model SteamEquipment # DF = 10

    PARAMETERS
    ext freesteam as CalcObject(Brief="Steam Physical Properties");

    VARIABLES
	in  In  as Stream;
	out Out as Stream;   
	
end

Model SteamEquipment_ph # DF = 8

    PARAMETERS
    ext freesteam as CalcObject(Brief="Steam Physical Properties");

    VARIABLES
	in  In  as Stream;
	out Out as Stream_ph;   

end

Model SteamEquipment_ps # DF = 8

    PARAMETERS
    ext freesteam as CalcObject(Brief="Steam Physical Properties");

    VARIABLES
	in  In  as Stream;
	out Out as Stream_ps;   

end
