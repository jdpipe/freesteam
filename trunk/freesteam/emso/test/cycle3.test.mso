#* 
	Open loop cycle model
	with all component model specified in terms of intrinsic parameters, 
	rather than intermediatate steam properties
	
	John Pye 2005 http://freesteam.sf.net
*#

using "streams";
using "turbine";
using "boiler";
using "pump";
using "condenser";
using "throttle";

FlowSheet CycleTest3
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
    DEVICES # 31 DF
    S1  as Stream_pT;  # 3
	BO	as Boiler_pT;     # 7
	TU	as Turbine;    # 7
	CO	as Condenser;  # 7
	PU 	as Pump;       # 7
	
	CONNECTIONS # BO -> TU -> CO -> TH -> BO  ... -25 DF
	S1      to  BO.In; # -5
	BO.Out	to	TU.In; # -5
	TU.Out	to  CO.In; # -5 
	CO.Out	to  PU.In; # -5
	#PU.Out	to  BO.In; # -5
	
	SPECIFY # -9 DF
	
	S1.mdot = 30 * "kg/s";
	S1.p = 60 * "bar";
	S1.T = (121 + 273.15) * "K";	

	# Boiler
	BO.Dp = -2 * "bar";
	BO.Out.T = (300 + 273.15) * "K";
	
	# Turbine
	TU.eta = 0.85;
	TU.Dp = -56 * "bar";
	
	# Condenser
	CO.Dp = -0.5 * "bar";
	CO.x = 0.00001;
	
	# Pump
	PU.eta = 0.65;
	PU.Out.p = 60 * "bar";

OPTIONS
	mode = "steady";
	outputLevel = "all";
end
