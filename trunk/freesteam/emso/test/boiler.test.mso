using "streams";
using "boiler";

FlowSheet BoilerTest # 0
		
	DEVICES
	S1 as Stream_pT; # 3
	BO as Boiler_pT; # 7
	
	CONNECTIONS # -5
	S1 to BO.In; 
	
	SPECIFY # -5
	S1.mdot = 38 * "kg/s";
	S1.p = 60 * "bar";
	S1.T = (121 + 273.15) * "K";	

	BO.Out.p = 60 * "bar";
	BO.Out.T = (500 + 273.15) * "K";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
	
end

FlowSheet BoilerTest2 # 0
		
	DEVICES
	S1 as Stream_pT; # 3
	BO as Boiler_pT; # 7
	
	CONNECTIONS # -5
	S1 to BO.In; 
	
	SPECIFY # -5
	S1.mdot = 38 * "kg/s";
	S1.p = 60 * "bar";
	S1.T = (121 + 273.15) * "K";	

	BO.Dp = -2 * "bar";
	BO.q = 140 * "MW";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
	
end
