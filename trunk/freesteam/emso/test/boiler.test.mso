using "streams";
using "boiler";

FlowSheet BoilerTest # 0
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
	DEVICES
	s as Stream_pT; # 3
	BO as Boiler; # 7
	
	CONNECTIONS # -5
	s to BO.In; 
	
	SPECIFY # -5
	s.mdot = 1 * "kg/s";
	s.T = 300 * "K";
	s.p = 10 * "bar";
	
	BO.Dp = -2 * "bar";
	BO.q = 3 * "kJ/s";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end

