using "streams";
using "turbine";

FlowSheet BoilerTest
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
	DEVICES
	s as Stream_ps;
	BO as Boiler;
	
	CONNECTIONS
	s to BO.In;
	
	SPECIFY
	s.mdot = 20000 * "kg/h";
	s.T = 300 * "K";
	s.p = 2 * "atm";
	
	BO.Out.p = 2 * "atm";
	BO.Out.T = 400 * "K";
	
	# BO.q = 50 * "GJ/h";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end

