using "pipe2";
	
FlowSheet Pipe2Test

	DEVICES
	S1 as Stream_pT;
	P as Pipe2;

	CONNECTIONS
	S1 to P.In;

	SET
	P.n = 3;
	P.D = 0.05 * "m";
	P.L = 20 * "m";
	P.eps = 1e-5 * "m";
	
	SPECIFY
	S1.p = 2 * "bar";
	S1.T = (273.15 + 25) * "K";
	
	P.Out.p = 1 * "bar";
	
	INITIAL
	P.X([2:P.n-1]).p = 1 * "bar";
	P.X([2:P.n-1]).u = 500 * "kJ/kg";
	P.X([2:P.n-1]).mdot = 0 * "kg/s";
	P.Out.mdot = 0 * "kg/s";
	
	OPTIONS
	outputLevel="all";
	
end
