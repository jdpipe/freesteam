using "steamequipment";

#* 
    Ideal (isentropic) turbine
*#
Model Turbine_ideal as SteamEquipment_ps # 8 + 1 - 3 = 6 DF

    VARIABLES
    P  as Power(Brief="Turbine Power");
	
    EQUATIONS # DF = -3
    "Mass Balance"
    In.mdot = Out.mdot;
    
    "Turbine power"
    P = In.mdot * In.h - Out.mdot * Out.h;
    
    "Isentropic constraint"
    Out.s = In.s;
end


#* 
    Turbine (non-isentropic, with efficiency specified)
*#
Model Turbine as SteamEquipment_ph # 8 + 2 - 3 = 7 DF

	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
    
    VARIABLES
    eta as Efficiency(Brief="Turbine efficiency"); # DF = 1
	h_es as SpecificEnthalpy(Default = 3000, Lower=150, Upper = 4300);
    #P as Power; # DF = 1
    
    EQUATIONS #DF = -3
    "Mass Balance"
    In.mdot = Out.mdot;
    
    #"Turbine power"
    #P = In.mdot * In.h - Out.mdot * Out.h;
    
    "Isentropic output enthalpy"
    h_es = freesteam.h_ps(Out.p, In.s);
    
    "Efficiency"
    (Out.h - In.h) = eta * (h_es - In.h);

end