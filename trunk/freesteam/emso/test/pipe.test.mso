#*
	First attempt at arrayised pipe 'chain'

	DOESN'T WORK
*#
FlowSheet PipeColebrookArrayTest1
	
	PARAMETERS
	n as Integer;
	
	DEVICES
	S1 as Stream_pT;
	PI(n) as PipeColebrook;
	
	EQUATIONS
	for i in [1:n-1]
		PI(i).Out.mdot = PI(i+1).In.mdot;
		PI(i).Out.h = PI(i+1).In.h;
		PI(i).Out.p = PI(i+1).In.p;
		PI(i).Out.T = PI(i+1).In.T;
		PI(i).Out.s = PI(i+1).In.s;
	end

	PI(1).Out.mdot = S1.mdot;
	PI(1).Out.h = S1.h;
	PI(1).Out.p = S1.p;
	PI(1).Out.T = S1.T;
	PI(1).Out.s = S1.s;

	S1.p = 1.5 * "bar";
	S1.T = (25 + 273.15) * "K";
	S1.mdot = 1 * "kg/s";
	
	for i in [1:n]
		PI(i).D = 0.025 * "m";
		PI(i).L = 15 * "m";
		PI(i).eps = 46e-6 * "m";
		PI(i).Qdot = 0 * "W";
	end

	SET n=3;
end

	

