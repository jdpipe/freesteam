using "streams";
using "condenser";

FlowSheet TestCondenser
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
	DEVICES
	S as Stream_pT;
	CO as Condenser_pT;
	#CO as Condenser;
	
	CONNECTIONS
	S to CO.In;
	
	SPECIFY
	S.mdot = 1 * "kg/s";
	#S.h = 3000 * "kJ/kg";
	S.p = 0.05 * "bar";
	S.T = 306 * "K";
	
	# CO.q = 1700 * "kJ/s";
	# CO.DT = -50 * "K";
	#CO.UA = 30 * "kW/K";
	#CO.DT = -50 * "K";
	CO.Out.p = 0.05 * "bar";
	CO.Out.T = (140 + 273.15) * "K";
	
	GUESS 
	# FIXME: [rafael] this is required when DT is specified instead of q:
	# CO.Out.h = 300 * "kJ/kg";
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end
