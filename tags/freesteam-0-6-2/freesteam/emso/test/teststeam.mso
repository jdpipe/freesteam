#*
    Test the steam library
*#

using "steam";

FlowSheet Test_Steam_ps
    
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");

	DEVICES
	S1 as Steam_ps;
	
	SPECIFY
	S1.p = 50.0 * "bar";
	S1.s = 5.0 * "kJ/kg/K";
	
    OPTIONS
    relativeAccuracy = 1e-10;
    mode = "steady"; 
    outputLevel = "all";
    
end


