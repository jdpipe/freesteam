using "streams";
using "turbine";

FlowSheet TestPump
	
	PARAMETERS
	freesteam as CalcObject(File="emsofreesteam");
	
	DEVICES
	S1 as Stream_waterT;
	PU as Pump;
	
	CONNECTIONS
	S1 to PU.In;
	
	SPECIFY
	"flow rate"
	S1.mdot = 20000 * "kg/h";
	
	"temperature"
	S1.T = 300 * "K";
	
	"efficiency"
	PU.eta = 0.9;
	
	OPTIONS
	mode = "steady";
	outputLevel = "all";
end
